exports[`renders correctly 1`] = `
<div
  className="bg-blue-dark w-full h-screen"
>
  <div
    className="flex justify-between pl-2 pr-2 md:pl-40 md:pr-40 md:pt-2"
  >
    <a
      href="/"
      onClick={[Function]}
    >
      <img
        alt="Reach"
        src="Logo.png"
      />
    </a>
    <img
      alt="close"
      className="hover:scale-125 hover:cursor-pointer"
      onClick={[Function]}
      src="close-menu.svg"
    />
  </div>
  <div
    className="p-24 flex flex-col items-center"
  >
    <h1
      className="text-base font-bold text-gray font-quicksand pb-6 md:text-xl"
    >
      Would you like to sign up as a:
    </h1>
    <div
      className="flex justify-evenly items-center"
    >
      <img
        alt="volunteer"
        className="rounded-xl"
        src="volunteer.jpg"
      />
      <h4
        className="text-xl font-bold text-gray font-quicksand pl-6 pr-6"
      >
        or
      </h4>
      <img
        alt="organization"
        className="rounded-xl"
        src="organization.jpg"
      />
    </div>
    <p
      className="flex flex-row text-base font-bold text-gray font-quicksand pt-6 md:text-xl"
    >
      Already have an account?
      <a
        href="/log-in"
        onClick={[Function]}
      >
        <p
          className="text-red pl-2 md:hover:scale-110"
        >
          Login here
        </p>
      </a>
    </p>
    <button
      className="text-base mt-4 h-12 w-72 rounded bg-red font-quicksand md:text-xl font-bold text-gray md:hover:scale-105"
    >
      Continue signing up
    </button>
  </div>
</div>
`;
